{
    "mcpServers": {
      "supabase": {
        "command": "npx",
        "args": [
          "-y",
          "@supabase/mcp-server-supabase@latest",
          "--access-token",
          "your-supabase-access-token-here"
        ]
      },
      "taskmaster-ai": {
        "command": "npx",
        "args": ["-y", "--package=task-master-ai", "task-master-ai"],
        "env": {
          "ANTHROPIC_API_KEY": "your-anthropic-api-key-here"
        }
      }
    }
} 